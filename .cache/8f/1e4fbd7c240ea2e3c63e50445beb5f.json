For=\"animal\">\n          <select\n            id=\"animal\"\n            value={animal}\n            onChange={(e) => setAnimal(e.target.value)}\n            onBlur={(e) => setAnimal(e.target.value)}\n          >\n            <option />\n            {ANIMALS.map((animal) => (\n              <option key={animal} value=\"animal\">\n                {animal}\n              </option>\n            ))}\n          </select>\n          <select\n            id=\"breed\"\n            value={breed}\n            onChange={(e) => setBreed(e.target.value)}\n            onBlur={(e) => setBreed(e.target.value)}\n          >\n            <option />\n            {breeds.map((breed) => (\n              <option key={breed} value=\"breed\">\n                {breed}\n              </option>\n            ))}\n          </select>\n        </label>\n        <button>Submit</button>\n      </form>\n      {pets.map((pet) => (\n        <Pet\n          name={pet.name}\n          animal={pet.animal}\n          breed={pet.breed}\n          key={pet.id}\n        />\n      ))}\n    </div>\n  );\n};\n\nexport default SearchParams;\n"},"lineCount":null}},"error":null,"hash":"0f067d6ef7d851250326f75dc9136601","cacheData":{"env":{}}}